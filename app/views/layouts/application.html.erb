<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= render "layouts/stylesheets" %>
  <%= javascript_include_tag "application" %>
  <%= render "layouts/icons" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>
<%= render "layouts/header" %>
<div class="container">
  <div class="content">
    <%= yield %>
  </div>
  <%= render "layouts/footer" %>
</div>
</body>
</html>
